{
  "name": "zombieplus-api",
  "version": "1.0.0",
  "description": "Zombie+ - Users API",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm run apidoc && npm run clusters",
    "doc": "npm run apidoc",
    "app": "babel-node app.js",
    "clusters": "babel-node clusters.js",
    "dev": "npm run apidoc && nodemon clusters.js --exec babel-node",
    "test": "NODE_ENV=test mocha test/**/*.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc"
  },
  "apidoc": {
    "name": "QA Ninja - API Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "author": "Fernando Papito",
  "dependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "consign": "^0.1.2",
    "cors": "^2.7.1",
    "date-fns": "^1.29.0",
    "express": "^4.17.3",
    "helmet": "^1.1.0",
    "jwt-simple": "^0.4.1",
    "moment": "^2.22.2",
    "morgan": "^1.6.1",
    "node-pre-gyp": "^0.14.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^2.0.0",
    "pg": "^7.9.0",
    "sequelize": "^5.2.8",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  }
}
